import java.util.Scanner;

public class QQClient {
	public static void main(String[] args) {
		Socket s = null;
		Scanner sc = new Scanner(System.in);
		String sout = null;
		String sint = null;
		try {
			s = new Socket("localhost",7777);
			sout = sc.next();
		} catch (UnknownHostException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
