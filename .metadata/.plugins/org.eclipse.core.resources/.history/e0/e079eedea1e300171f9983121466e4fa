package IO;

import java.io.File;

public class TestCtrlF {
	public static void main(String[] args) {
		String filename = "x3";
		String parentName = "D:/haha";
		printDir(parentName);
	}
	
	static void printDir(String parentName){
		File f = new File(parentName);
		File fl [] = f.listFiles();
		for (File file : fl) {
			if(file.isDirectory()){
				printDir(file.getAbsolutePath());
			}
				System.out.println(file);
			
			
			
			
		}
	}
}
