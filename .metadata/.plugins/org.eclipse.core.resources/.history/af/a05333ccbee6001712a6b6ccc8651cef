		for (int i = 0; i < 100; i++) {
//			try {
//				Thread.sleep(300);
//			} catch (InterruptedException e) {
//				// TODO Auto-generated catch block
//				e.printStackTrace();
//			}
			if(i==30){
				Thread.yield();
				System.out.println("ÈÃ³öCPU");
				Thread.yield();
				Thread.yield();
				Thread.yield();
				Thread.yield();
				Thread.yield();
				Thread.yield();
				Thread.yield();
			}
			
			System.out.println("c"+i);
		}
	}
	
}
