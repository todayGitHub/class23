package Thread;

public class B extends Thread{
	int a ;
	public B(int a){
		this.a = a;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for (int i = 0; i < 10; i++) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println("ÕýÔÚÉÕË®"+a);
		}
	}

	
}