package IO;

import java.io.File;
import java.io.IOException;

public class TestFile {
	public static void main(String[] args) throws IOException {
		File f = new File("C:/1.txt");
		f.createNewFile();	
		File f2 = new File("C:/1");
		f2.createNewFile();
		f2.mkdir();
		//java中的File类从来就不是文件的意思；路径类
		
		
	}
	
}
