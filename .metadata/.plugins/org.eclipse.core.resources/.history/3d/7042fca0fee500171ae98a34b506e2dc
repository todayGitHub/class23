package Thread;

public class Test {
	public static void main(String[] args) throws InterruptedException {		
		haha("a");
		A a = new A();
		a.start();
		
	}
	public static void haha(String a) throws InterruptedException{
		
	
		for (int i = 0; i < 10; i++) {
			Thread.sleep(1000);
			System.out.println(a);
		}
	}
	
}
