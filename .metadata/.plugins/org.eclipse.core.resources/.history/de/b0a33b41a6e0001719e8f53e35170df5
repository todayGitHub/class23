package Collection.list;

import java.util.Date;

public class Test {
	public static void main(String[] args) {
		ArrayList al = new ArrayList();
		System.out.println(al.isEmpty());
		System.out.println(al.size());
		al.add(1);
		al.add("$");
		
		al.add(new Date());
		
		System.out.println(al.isEmpty());
	}
}
